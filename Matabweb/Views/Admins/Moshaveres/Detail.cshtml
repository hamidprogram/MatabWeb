@model BE.Moshavere

@{
    ViewData["Title"] = "Detail";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}





<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1>جداول مشاوره ها</h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-left">
                        <li class="breadcrumb-item"><a href="@Url.Action("Index","Admins",new {IsRegister = true})">داشبورد</a></li>
                        <li class="breadcrumb-item active">مدیریت مشاوره ها</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <!-- /.card-header -->
                    <div class="card-body">

                        <div>
                            <dl class="row">
                                <dt class="col-sm-2">
                                    نام و نام خانوادگی
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.FullName)
                                </dd>
                                <dt class="col-sm-2">
                                    شماره تلفن
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.PhoneNumber)
                                </dd>
                                <dt class="col-sm-2">
                                    موضوع
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Subject)
                                </dd>
                                <dt class="col-sm-2">
                                    توضیحات
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Descreption)
                                </dd>
                            </dl>
                        </div>
                        <div>
                            <a asp-action="DeleteM" asp-route-id="@Model.ID"><i class="fa fa-remove"></i></a> |
                            <a asp-action="Moshaveres"><i class="fa fa-backward"></i></a>
                        </div>

                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </section>
    <!-- /.content -->
</div>